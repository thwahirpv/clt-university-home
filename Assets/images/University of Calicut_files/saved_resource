		thumbnails: false,
		time: 3000,
		transPeriod: 1000,
		cols: 2,
		rows: 1,
		slicedCols: 1,
		slicedRows: 1,
		fx: "simpleFade",
		gridDifference: 250,
		height: "26.6%%",
		minHeight: "85pxpx",
		imagePath: "/modules/mod_blz_responsive_slider/images/",
